{
  "name": "interpolate-expression",
  "version": "1.0.1",
  "description": "Replace `{{expression}}` in a string with the evaluated value of the expression",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "nyc mocha test/*.test.ts test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnArrowwood/interpolate-expression.git"
  },
  "author": "John Arrowwood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohnArrowwood/interpolate-expression/issues"
  },
  "homepage": "https://github.com/JohnArrowwood/interpolate-expression#readme",
  "dependencies": {
    "expression-eval": "^1.1.0",
    "jsep": "^0.3.2",
    "progressive-eval": "^1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.9",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.0.1",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  }
}
